.container
  = render 'shared/header'
  
  #description
    =t('app_description').html_safe
  
  
  #import_stats
    %p= t('app_stats', import_count: @count, unique_state_count: @unique_state_count, record_count: @record_count)
  
  .row
    .col-xs-12.col-md-6#new-import
      %h3 Upload new Voter File Extract
      = render 'form'
    .col-xs-12.col-md-6#run-report
      %h3= t('matching_report.header')
      = link_to reports_path do 
        %button= t('matching_report.button') 
